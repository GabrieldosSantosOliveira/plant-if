{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/assets/*": ["./src/assets/*"],
      "@/components/*": ["./src/components/*"],
      "@/constants/*": ["./src/constants/*"],
      "@/contexts/*": ["./src/contexts/*"],
      "@/errors/*": ["./src/errors/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/infra/*": ["./src/infra/*"],
      "@/interfaces/*": ["./src/interfaces/*"],
      "@/main/*": ["./src/main/*"],
      "@/screens/*": ["./src/screens/*"],
      "@/services/*": ["./src/services/*"],
      "@/styles/*": ["./src/styles/*"],
      "@/test/*": ["./test/*"],
      "@/jest/*": ["./jest/*"]
    }
  }
}
