{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/constants/*": ["./src/constants/*"],
      "@/domain/*": ["./src/domain/*"],
      "@/errors/*": ["./src/errors/*"],
      "@/helpers/*": ["./src/helpers/*"],
      "@/infra/*": ["./src/infra/*"],
      "@/interfaces/*": ["./src/interfaces/*"],
      "@/main/*": ["./src/main/*"],
      "@/models/*": ["./src/models/*"],
      "@/services/*": ["./src/services/*"],
      "@/ui/*": ["./src/ui/*"],
      "@/test/*": ["./test/*"],
      "@/jest/*": ["./jest/*"]
    }
  }
}
