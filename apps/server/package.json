{
  "name": "server",
  "version": "1.0.0",
  "scripts": {
    "dev": "tsnd -r tsconfig-paths/register ./src/main/server",
    "start": "node ./dist/main/server",
    "build": "rimraf ./dist && babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --passWithNoTests --verbose --colors",
    "test:watch": "npm run test -- --watch",
    "test:staged": "npm run test -- --bail=0 --findRelatedTests",
    "test:ci": "npm run test -- --coverage",
    "type:check": "rimraf ./dist && tsc --noEmit",
    "format": "prettier -w ."
  },
  "keywords": [],
  "author": "Gabriel dos Santos Oliveira <gabrielsantosoliveira951@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@faker-js/faker": "^8.0.2",
    "@rocketseat/eslint-config": "^2.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.2",
    "@types/nodemailer": "^6.4.8",
    "@types/speakeasy": "^2.0.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.1",
    "prettier": "3.2.5",
    "prisma": "^5.0.0",
    "rimraf": "^5.0.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.4",
    "speakeasy": "^2.0.0",
    "zod": "^3.21.4"
  },
  "engines": {
    "node": "18.x"
  }
}
